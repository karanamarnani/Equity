import java.util.ArrayList;

public class ABCDPatternRetracements {

	private Float BCRetracement;
	private Float CDRetracement;
	private ArrayList<ABCDPatternRetracements> retracementList;

	public ArrayList<ABCDPatternRetracements> getRetracementList() {
		return retracementList;
	}

	public ABCDPatternRetracements() {

		this.retracementList = new ArrayList<>();
		this.retracementList.add(new ABCDPatternRetracements(0.382f, 2.24f));
		this.retracementList.add(new ABCDPatternRetracements(0.382f, 2.618f));
		this.retracementList.add(new ABCDPatternRetracements(0.5f, 2f));
		this.retracementList.add(new ABCDPatternRetracements(0.618f, 1.272f));
		this.retracementList.add(new ABCDPatternRetracements(0.707f, 1.41f));
		this.retracementList.add(new ABCDPatternRetracements(0.786f, 1.272f));
		this.retracementList.add(new ABCDPatternRetracements(0.886f, 1.13f));

	}

	public ABCDPatternRetracements(Float bCRetracement, Float cDRetracement) {
		this.BCRetracement = bCRetracement;
		this.CDRetracement = cDRetracement;
	}

	public Float getBCRetracement() {
		return BCRetracement;
	}

	public void setBCRetracement(Float bCRetracement) {
		BCRetracement = bCRetracement;
	}

	public Float getCDRetracement() {
		return CDRetracement;
	}

	public void setCDRetracement(Float cDRetracement) {
		CDRetracement = cDRetracement;
	}

}
